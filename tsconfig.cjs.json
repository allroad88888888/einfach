{
    "compilerOptions": {
        "rootDir": "src",
        "outDir": "cjs",
        "declaration": false,
        "target": "ESNext",
        "module": "CommonJS",
        "jsx": "react-jsx",
        "strict": true,
        "skipLibCheck": true,
        "noEmitHelpers": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
    },
    "exclude": [
        "node_modules",
        "**/*.test.ts",
        "**/*.test.tsx",
        "**/test"
    ]
}